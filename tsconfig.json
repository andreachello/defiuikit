{
      "compilerOptions": {
        "jsx": "react",
        "target": "ES2015",
        "module": "ES2020",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
      "moduleResolution": "node",
    "skipLibCheck": true,                                 /* Skip type checking all .d.ts files. */
    "resolveJsonModule": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
